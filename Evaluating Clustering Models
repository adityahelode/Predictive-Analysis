df = pd.read_csv("IoT_Devices.csv")
df.head()

#Information regarding dataset
df.info()

#Dropping Null Values
df = df.dropna()

objList = df.select_dtypes(include = "object").columns
print (objList)

#Label Encoding
L = LabelEncoder()
for feat in objList:
    df[feat] = L.fit_transform(df[feat].astype(str))
print (df.info())
